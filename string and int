void BigLittleEndian()
{
   string x = "1234";
   //string insert, be carful that must"", not ''
   x.insert(0,"45");
   cout<<"test:"<<x<<endl;
   //string -> int
   int test = atoi(x.c_str());
   //every bit,be careful '' not ""
   int test1 = x[0]-'0';
   cout<<"test:"<<test<<endl<<"test1:"<<test1<<endl;
   int a = 10;
   //int - > string #include<string>
   string test2 = to_string(a);
}
